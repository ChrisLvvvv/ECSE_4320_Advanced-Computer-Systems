[global]
ioengine=libaio
direct=1
size=1G
filename=../ssdtestfile
time_based=1
runtime=30
group_reporting=1

[qd1-randread]
stonewall
iodepth=1
rw=randread
bs=4k

[qd1-randwrite]
stonewall
iodepth=1
rw=randwrite
bs=4k

[qd1-seqread]
stonewall
iodepth=1
rw=read
bs=128k

[qd1-seqwrite]
stonewall
iodepth=1
rw=write
bs=128k
